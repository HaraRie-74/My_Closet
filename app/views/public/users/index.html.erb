<div class="container">
  <div class="row">
    <h1>User一覧</h1>
    <%= form_with url:users_search_path, method: :get do |f| %>
  <%= f.text_field :content %>
  <%= f.hidden_field :model, :value => 'user' %>
  <%= f.submit "検索" %>
<% end %>
  </div>
  <%= render 'user_index', users: @users %>
</div>